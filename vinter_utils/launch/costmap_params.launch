<launch>
	<param name="costmap_node/costmap/robot_base_frame" value="intersection_0"/>
	<param name="costmap_node/costmap/update_frequency" value="10"/>
	<param name="costmap_node/costmap/publish_frequency" value="10"/>
	<param name="costmap_node/costmap/obstacle_range" value="10"/>
	<param name="costmap_node/costmap/raytrace_range" value="15"/>
	
	<param name="costmap_node/costmap/inflation_radius" value="0.05"/>
	<param name="costmap_node/costmap/robot_radius" value="0.03"/>	
	
	<param name="costmap_node/costmap/observation_sources" value="base_0 base_1 base_2 base_3"/>
	
	<!-- North East Base laser -->
	<param name="costmap_node/costmap/base_0/topic" value="base_0/base_scan" />
	<param name="costmap_node/costmap/base_0/data_type" value="LaserScan" />
	<param name="costmap_node/costmap/base_0/observation_persistence" value="0.0" />
	<param name="costmap_node/costmap/base_0/obstacle_range" value="10" />
	<param name="costmap_node/costmap/base_0/raytrace_range" value="15" />
	<param name="costmap_node/costmap/base_0/clearing" value="true" />
	
	<!-- South East Base laser -->
	<param name="costmap_node/costmap/base_1/topic" value="base_1/base_scan" />
	<param name="costmap_node/costmap/base_1/data_type" value="LaserScan" />
	<param name="costmap_node/costmap/base_1/observation_persistence" value="0.0" />
	<param name="costmap_node/costmap/base_1/obstacle_range" value="10" />
	<param name="costmap_node/costmap/base_1/raytrace_range" value="15" />
	<param name="costmap_node/costmap/base_1/clearing" value="true" />
	
	<!-- South West Base laser -->
	<param name="costmap_node/costmap/base_2/topic" value="base_2/base_scan" />
	<param name="costmap_node/costmap/base_2/data_type" value="LaserScan" />
	<param name="costmap_node/costmap/base_2/observation_persistence" value="0.0" />
	<param name="costmap_node/costmap/base_2/obstacle_range" value="10" />
	<param name="costmap_node/costmap/base_2/raytrace_range" value="15" />
	<param name="costmap_node/costmap/base_2/clearing" value="true" />
	
	<!-- North West Base laser -->
	<param name="costmap_node/costmap/base_3/topic" value="base_3/base_scan" />
	<param name="costmap_node/costmap/base_3/data_type" value="LaserScan" />
	<param name="costmap_node/costmap/base_3/observation_persistence" value="0.0" />
	<param name="costmap_node/costmap/base_3/obstacle_range" value="10" />
	<param name="costmap_node/costmap/base_3/raytrace_range" value="15" />
	<param name="costmap_node/costmap/base_3/clearing" value="true" />
	
	<!-- param name="costmap_node/costmap/static_map" value="true"/> -->
	<param name="costmap_node/costmap/publish_voxel_map" value="true"/>
	<param name="costmap_node/costmap/track_unknown_space" value="true"/>
</launch>